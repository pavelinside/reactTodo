{
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "globals": {
    "window": true,
    "document": true,
    "console": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": 0,
    "indent": ["warn", 2],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-debugger":"off"
  },
  "plugins": [
    "react"
  ]
}